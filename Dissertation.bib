journal = {COCS '90 Proceedings of the ACM SIGOIS and IEEE CS TC-OA conference on Office information systems},
keywords = {computer supported cooperative work,real time conferencing,shared window systems},
number = {2-3},
pages = {227--237},
title = {{Sharing views and interactions with single-user applications}},
