@article{Zhou2006,
abstract = {Intelligence analysis is a process of evaluating and transforming raw data into descriptions, explanations, and conclusions for intelligence consumers. It requires exploring multiple competing hypotheses in an attempt to formulate a more realistic model of the world to better describe or explain current events or predict likely future events. For example, government analysts may hypothesize the capabilities and intentions of terrorist groups to understand the threat of possible terror attacks; financial analysts may examine business and market information to evaluate potential investment candidates. An intelligence analyst is an individual who performs intelligence analysis of a given situation. Intelligence analysis is a very difficult task because of the complexity of the problems addressed and the inherent limitations in human cognitive processes [1]. First, intelligence analysts must deal with much uncertainty due to often incomplete and inconsistent information. Second, human analysts are likely to form cognitive biases toward a particular hypothesis. The difficulty of the task increases when analysts must deal with large amounts of dynamically updated, transient information (e.g., surveillance videos). To tackle such a difficult task, analysts normally take a targetcentric approach [2]. Specifically, analysts first formulate their hypotheses, and then validate the hypotheses by gathering and distilling information from various sources. During a hypothesis validation process, relevant information would be retrieved and marked as evidence, which either supports or refutes a hypothesis. As intelligence analysis becomes an increasingly important area for many sectors (e.g., government, finance, and pharmaceuticals), researchers and practitioners have developed a wide range of computer technologies and tools to aid human analysts in performing their tasks. However, most of the efforts focus on information search (e.g., QA-based information retrieval) or information analytics (e.g., data mining and video analysis). Limited attention has been devoted to providing better user interfaces through which human analysts can better manage a complex analysis process. With the current far-from-perfect information search and analytic technologies, we believe that it is critical to create a ``smart'' interactive environment where human analysts and computers can work collaboratively. On the one hand, computers use the human analysis process as a context to better understand the needs of analysts and assist them in their tasks (e.g., reminding them to investigate competitive hypotheses). On the other hand, human analysts could use computer-presented information as a context to better articulate their needs (e.g., issuing follow-up inquiries) and manage their analysis process (e.g., better organizing hypotheses and marshaling evidence). The purpose of this workshop is to bring together researchers and practitioners who are interested in developing and applying the state-of-art intelligent user interface (IUI) technologies to enhance intelligence analysis. Ideally, the developed IUI technologies or tools can capitalize on human strength (e.g., reasoning and detecting visual patterns) and leverage machine automation to overcome scaling and complexity challenges.},
author = {Zhou, Michelle X and Maybury, Mark},
doi = {10.1145/1111449.1111456},
file = {:Users/dong/Downloads/p16-zhou.pdf:pdf},
isbn = {1-59593-287-9},
journal = {IUI '06: Proceedings of the 11th international conference on Intelligent user interfaces},
pages = {16},
title = {{Intelligent user interfaces for intelligence analysis}},
url = {http://doi.acm.org/10.1145/1111449.1111456},
year = {2006}
}
